{
    "name": "Title Picker",
    "description": "A title picker extension",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/images/clipart1120729.png",
        "32": "/images/clipart1120729.png",
        "48": "/images/clipart1120729.png"
    }
}